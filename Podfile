# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'
use_frameworks!

target 'AFCC' do

  # Pods for AFCC
  pod 'Firebase'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'SwiftKeychainWrapper', '~> 3.0'
  pod 'Firebase/Storage'
  pod 'FSCalendar'

  target 'AFCCTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AFCCUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
